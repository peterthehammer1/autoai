{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/regenerate-slots",
      "schedule": "0 3 * * 1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
